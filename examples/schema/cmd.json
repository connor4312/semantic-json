{
	"$namespaces": {
		"t": "json-types.org/basic-types",
		"td": "json-types.org/type-definition",
		"cmd": "json-cmd.hediet.de"
	},
	"$type": "td#TypeDefinitions",
	"packageId": "demo.org/bla",
	"typeDefinitions": {
		"ContactBook": { "kind": "union", "of": ["#CmdPrint"] },
		"CmdPrint": {
			"kind": "object",
			"allowUnknownProperties": true,
			"properties": {
				"files": {
					"cmd#kind": "PositionalArg",
					"optional": true,
					"defaultValue": "",
					"type": [
						"t#String",
						{
							"$type": "t#RegexConstraint",
							"regex": ".*"
						},
						"t#FilePath"
					]
				},
				"printContent": {
					"type": "t#Boolean"
				}
			}
		}
	}
}
